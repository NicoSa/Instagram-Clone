<%= form_for @post, html: { multipart: true } do |f| %>

    <%= f.label :comment %>
	<%= f.text_field :comment %>

    <%= f.label :tag_names, 'Tags' %>
	<%= f.text_field :tag_names %>

    <%= f.label :picture %>
	<%= f.file_field :picture %>
  
  	<%= f.submit 'Add' %>
<% end %>
